<h1>Benutzer</h1>

{{#paper-list}}
  {{#each model as |user|}}
    {{#paper-item class="md-3-line"}}
      <img src={{user.gravatarUrl}} alt={{user.name}} class="md-avatar">
      <div class="md-list-item-text">
        <h3>{{user.name}}</h3>
        <h4>{{user.email}}</h4>
        <p>Some longer notes here could be here. But they aren't.</p>
      </div>
    {{/paper-item}}
  {{/each}}
{{/paper-list}}
